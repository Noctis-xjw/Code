
#define _CRT_SECURE_NO_WARNINGS 1

#include<windows.h>
#include <stdio.h>

int my_strlen (const char *str )
{
       const char *start = str;
       while (*str )
      {
             str++;
      }
       return str -start;
}

int main ()
{
       char *str = "hello world";
       printf("%d" ,my_strlen( str));
       system("pause" );
       return 0;
}
